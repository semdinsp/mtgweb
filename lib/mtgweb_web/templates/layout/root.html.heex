    <meta property="og:url" content={assigns[:og_url] || "https://mtg-consulting.net"}>
    <meta property="og:type" content="website">
    <meta property="og:site_name" content="MTG Consulting">
    
    <!-- Twitter Cards -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content={assigns[:og_title] || "MTG Consulting - Professional Accounting & CFO Services"}>
    <meta name="twitter:description" content={assigns[:og_description] || "Expert accounting, bookkeeping, and CFO services for growing businesses."}>
    <meta name="twitter:image" content="https://mtg-consulting.net/images/mtglogo.jpg">
    
    <!-- Structured Data (JSON-LD) -->
    <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "AccountingService",
      "name": "MTG Consulting",
      "description": "Professional CPA and QuickBooks consulting for small businesses. Expert accounting, bookkeeping, and CFO services with free consultation.",
      "url": "https://mtg-consulting.net",
      "telephone": "+1-503-931-4045",
      "email": "michael@mtg-consulting.net",
      "address": {
        "@type": "PostalAddress",
        "addressLocality": "Portland",
        "addressRegion": "OR",
        "addressCountry": "US"
      },
      "serviceArea": "United States",
      "priceRange": "$250-$995",
      "founder": {
        "@type": "Person",
        "name": "Michael Grandinetti",
        "jobTitle": "CPA, Managing Partner"
      },
      "employee": [
        {
          "@type": "Person",
          "name": "Michael Grandinetti",
          "jobTitle": "CPA, Managing Partner"
        },
        {
          "@type": "Person",
          "name": "Scott Sproule",
          "jobTitle": "Technology and Integration Specialist"
        },
        {
          "@type": "Person",
          "name": "Carmella Sawali",
          "jobTitle": "CPA, Audit and Accounting"
        }
      ],
      "service": [
        {
          "@type": "Service",
          "name": "Basic Bookkeeping",
          "description": "Data entry, bank reconciliation and monthly reports",
          "offers": {
            "@type": "Offer",
            "price": "250",
            "priceCurrency": "USD",
            "billingIncrement": "P1M"
          }
        },
        {
          "@type": "Service",
          "name": "Enhanced Bookkeeping",
          "description": "Basic services plus accounts receivable, payable and payroll",
          "offers": {
            "@type": "Offer",
            "price": "500",
            "priceCurrency": "USD",
            "billingIncrement": "P1M"
          }
        },
        {
          "@type": "Service",
          "name": "Comprehensive Bookkeeping",
          "description": "Enhanced services plus financial analysis, tax prep and advisory",
          "offers": {
            "@type": "Offer",
            "price": "995",
            "priceCurrency": "USD",
            "billingIncrement": "P1M"
          }
        }
      ],
      "aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": "5.0",
        "reviewCount": "25"
      }
    }
    </script>
    
